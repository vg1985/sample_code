<div class='row'>
	<div class="col-md-6">
		<h4>Local Inbound DID Rates</h4>
		<div class="well form-horizontal form-bordered">
    		<%=f.fields_for :local_did_rate do |ldr|%>
				<div class="form-group local_activation_spinner_group">
					<label class="col-sm-3 control-label">Activation</label>
					<div class="col-sm-4">
						<%=ldr.text_field :activation, id: 'local_activation_spinner', title: t(:activation_helptext), data: {placement: 'top', toggle: 'tooltip'}, class: 'tooltips', style: 'width:75%'%>
					</div>
					<label class="col-sm-3 control-label">Use Default</label>
	                <div class="col-sm-2 control-label">
	                	<div class="toggle-default" id="local_activation_tgl" data-spinner="local_activation_spinner"></div>
	                	<input type="checkbox" style="opacity:0; position:absolute;" name="local_activation_tgl_val" id="local_activation_tgl_chkbox">
	                </div>
				</div>
				<div class="row">
					<div class="col-sm-offset-3">
						<span class="error" id="local_activation_spinner_error"></span>
					</div>
				</div>
				<div class="form-group local_monthly_spinner_group">
					<label class="col-sm-3 control-label">Monthly</label>
					<div class="col-sm-4">
