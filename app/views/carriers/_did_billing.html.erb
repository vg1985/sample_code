				</div>
				<div class="row">
					<div class="col-sm-offset-3">
						<span class="error" id="local_monthly_spinner_error"></span>
					</div>
				</div>
				<div class="form-group local_permin_spinner_group">
					<label class="col-sm-3 control-label">Per Minute</label>
					<div class="col-sm-4">
						<%=ldr.text_field :per_minute, id: 'local_permin_spinner', title: t(:pmin_helptext), data: {placement: 'top', toggle: 'tooltip'}, class: 'tooltips', style: 'width:75%'%>
