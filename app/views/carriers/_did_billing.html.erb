				<div class="row">
					<div class="col-sm-offset-3">
						<span class="error" id="local_permin_spinner_error"></span>
					</div>
				</div>
				<div class="form-group local_billstart_spinner_group">
					<label class="col-sm-3 control-label">Bill Start</label>
					<div class="col-sm-4">
						<%=ldr.text_field :bill_start, id: 'local_billstart_spinner', title: t(:bstart_helptext), data: {placement: 'top', toggle: 'tooltip'}, class: 'tooltips', style: 'width:75%'%>
